<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="classcubby.com.clickpad">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CLEAR_APP_CACHE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Receptionist_Dashboard_Activity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Receptionist.Dashboard.Patientsearchinformation.Dashboard_newpatient_search_Activity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Receptionist.Dashboard.Editpatientinformation.Edit_PatientInformation_Activity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Receptionist.Dashboard.Addpatientinformation.Add_PatientInformation_Activity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Receptionist.Dashboard.Addpatientinformation.Questionarieeadapter.Addpatient_Questionariee_Activity" android:screenOrientation="portrait"></activity>

    </application>

</manifest>